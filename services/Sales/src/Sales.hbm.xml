<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class entity-name="com.wavemaker.testcharts203.sales.SalesData" table="SalesData">
        <id name="id" type="integer">
            <column name="Id" length="255" not-null="true" precision="19"/>
            <generator class="identity"/>
        </id>
        <property name="name" type="string">
            <column name="Name" length="255" not-null="false" precision="19"/>
        </property>
        <property name="sales" type="integer">
            <column name="Sales" length="255" not-null="false" precision="19"/>
        </property>
        <property name="orders" type="integer">
            <column name="Orders" length="255" not-null="false" precision="19"/>
        </property>
        <property name="year" type="date">
            <column name="Year" length="0" not-null="false" precision="19"/>
        </property>
    </class>
</hibernate-mapping>